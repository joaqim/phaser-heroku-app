{
  "compilerOptions": {
    "target": "ESNext",
    "module": "CommonJS",
     "lib": [
       "dom",
       "ES2015",
       "ESNext"
     ],
    "strict": true,
    "noImplicitAny": false,
     "strictPropertyInitialization": false,
    "paths": {
      "*": [
        "./node_modules",
        "./src/frontend"
      ]
    },
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
